   "source": [
    "t = int(input())\n",
    "dp = [0]*101\n",
    "dp[1] = 1; dp[2] = 1; dp[3] = 1; dp[4] = 2; dp[5] = 2\n",
    "\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    if dp[n] != 0:\n",
    "        print(dp[n])\n",
    "    else:\n",
    "        for i in range(6, n+1):\n",
    "            dp[i] = dp[i-5] + dp[i-1]\n",
    "        print(dp[n])\n"
   ]
