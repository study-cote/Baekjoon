   "source": [
    "n = int(input())\n",
    "dp = []\n",
    "for _ in range(n):\n",
    "    dp.append(list(map(int, input().split())))\n",
    "\n",
    "for i in range(n-1, 0, -1):\n",
    "    for j in range(i):\n",
    "        dp[i-1][j] = max(dp[i][j], dp[i][j+1]) + dp[i-1][j]\n",
    "print(dp[0][0])"
   ]
