    "t = int(input())\n",
    "\n",
    "for _ in range(t):\n",
    "    k = int(input())\n",
    "    n = int(input())\n",
    "    \n",
    "    list = [i for i in range(1, n+1)] # 0층 \n",
    "    \n",
    "    for _ in range(k):\n",
    "        for j in range(1, n):\n",
    "            list[j] += list[j-1]\n",
    "    \n",
    "    print(list[-1]) # 리스트의 마지막 수"
   ]
