   "source": [
    "a = int(input())\n",
    "\n",
    "sum = 0\n",
    "n = 1\n",
    "\n",
    "while a:\n",
    "    sum += n\n",
    "    \n",
    "    if a <= sum:\n",
    "        break\n",
    "    \n",
    "    n += 1\n",
    "\n",
    "b = a - (sum - n) # 입력값 - 전 단계 분수들의 개수 = 몇 번째 값인지 알 수 있음\n",
    "\n",
    "if n % 2 == 0:\n",
    "    print('%d/%d'%(b, n+1-b)) # 짝수일때 왼쪽\n",
    "else:\n",
    "    print('%d/%d'%(n+1-b, b)) # 홀수일때 오른쪽\n",
    "    "
   ]
