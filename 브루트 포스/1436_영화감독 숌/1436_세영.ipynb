    "n = int(input())\n",
    "cnt = 0\n",
    "s = 666 \n",
    "\n",
    "while True:\n",
    "    if '666' in str(s): # 문자열 '666'이 포함되어 있으면 count\n",
    "        cnt += 1 \n",
    "    if cnt == n:\n",
    "        print(s); break\n",
    "    s += 1"
